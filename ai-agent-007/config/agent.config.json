{
  "agent": {
    "name": "AI Agent 007",
    "version": "0.1.0",
    "mode": "autonomous"
  },
  "ai": {
    "provider": "openai",
    "model": "gpt-4-turbo-preview",
    "temperature": 0.7,
    "maxTokens": 4000
  },
  "execution": {
    "enableExecution": true,
    "sandboxMode": true,
    "timeoutSeconds": 300,
    "maxIterations": 10
  },
  "codeGeneration": {
    "languages": ["javascript", "typescript", "python", "html", "css"],
    "frameworks": {
      "web": ["react", "vue", "svelte"],
      "backend": ["express", "fastapi", "flask"],
      "css": ["tailwind", "styled-components"]
    },
    "style": {
      "indentation": 2,
      "quotes": "single",
      "semicolons": true
    }
  },
  "review": {
    "enableSecurityScan": true,
    "enableQualityCheck": true,
    "enablePerformanceCheck": true,
    "minimumScore": 70
  },
  "output": {
    "directory": "./output",
    "includeTests": true,
    "includeDocumentation": true
  },
  "logging": {
    "level": "info",
    "logFile": "./logs/agent.log",
    "enableConsole": true
  }
}
